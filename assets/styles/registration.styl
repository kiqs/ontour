
body
	background #c1c6c9

#registration
	position relative
	width 500px
	height 600px
	margin 10% 35%
	background #0c1f38

#email, #password
	position relative
	top 50px
	left 50px
	width 80%
	line-height 25pt
	font $font
	display block
	padding 12px
	outline none
	border 0px

#register
	position relative
	top 20px
	left 50px
	width 30%
	cursor pointer
	padding $padding
	width 150px
	line-height 25pt
	text-align center
	font $font
	display block
	color white
	background green